{% if items %}
    <ul class="pagination text-center" aria-label="Pagination">
        <li class="pagination-previous{% if current == 1 %} disabled{% endif %}">
            {% if current > 1 %}
            <a class="pagination-prev" href="{{ items.previous.href }}" title="{{ 'Go to previous page'|t }}" rel="prev"{{ items.previous.attributes|without('href', 'title', 'rel') }}>
            {% endif %}
                <span class="icon-du-left-arrow" aria-hidden="true"></span>
                <span class="show-for-sr">Previous</span>
            {% if current > 1 %}
            </a>
            {% endif %}
        </li>
        {% for key, item in items.pages %}
            <li{% if key == current %} class="current"{% endif %}>
                {% if key == current %}
                    <span class="show-for-sr">You're on page</span> {{ current }}
                {% else %}
                    <a href="{{ item.href }}" aria-label="Page {{ key }}">{{ key }}</a></li>
                {% endif %}
            <li>
        {% endfor %}

        <li class="pagination-next">
            <a href="{{ items.next.href }}" aria-label="Next page">
                <span class="icon-du-right-arrow" aria-hidden="true"></span>
                <span class="show-for-sr">Next</span>
            </a>
        </li>
    </ul>
{% endif %}
