<article{{ attributes|without('about', 'role') }} id="main-content" role="main" >
    <div class="row">
        <div class="columns large-offset-4 large-8 pull-up page-title-band">
            {% block page_title %}
                {% include "@atoms/headline.twig" with {'title' : "This is the Page Title", 'type' : 'h1'} %}
            {% endblock %}
        </div>
    </div>
    <div class="row">
        <div class="columns small-12 large-3 pull-up">
            {% block sub_nav_block %}
                {% include "@molecules/sub-nav.twig" %}
            {% endblock %}
        </div>
        <div class="columns small-12 large-8 large-offset-1 intro">
            {% block page_overview %}
                {% include "@molecules/page-overview.twig" %}
            {% endblock %}
        </div>
    </div>
    {% block page_content %}
         {{ content }}
    {% endblock %}
</article>
