{% include "organisms-push-menu" %}
<div class="off-canvas-content" data-off-canvas-content>
	{% include "organisms-header" %}
		<article id="main-content" role="main">
			<div class="row">
				<div class="columns large-offset-4 large-8 pull-up page-title-band">
					{% if page_title %}
						{% include "atoms-headline" %}
					{% endif %}
				</div>
			</div>
			{% if search_enabled %}<script async type="text/javascript" src="/_duresources/foundation/js/google-search.js"></script>{% endif %}
			<div class="row">
				<div class="columns small-12 large-12">
					{% if sub_menu %}
						{% include "molecules-sub-nav" %}
					{% endif %}
				</div>
				<div class="columns small-12 large-10 large-offset-1">
					{% if search_results %}
						{{ listing_summary|raw }}
						<ol class="search-results {% if search_enabled %}hide{% endif %}">

							{% if keyword_items %}
								<li class="search-result--keyword-hit">
									<a href="#">
										{% include "atoms-headline" %}
										<p class="search-result--url">{{ url }}</p>
									</a>
								</li>
							{% endif %}

							{% if search_items %}
								<li>
									<a href="#">
										{% include "atoms-headline" %}
										<p><span class="search-result--type">{{ search_type }} |</span> {{ description|raw }}</p>
										<p class="search-result--url">{{ url }}</p>
									</a>
								</li>
							{% endif %}
						</ol>

						{% if search_enabled %}
							
							<div class="search-results__no-results hide">

								<p>No pages were found containing <strong>&ldquo;<span class="term"></span>&rdquo;</strong>.</p>
								
								<p>Suggestions:</p>
								
								<ul>
									<li>Make sure all words are spelled correctly.</li>
									<li>Try different keywords</li>
									<li>Try more general keywords</li>
								</ul>

							</div>

							<div class="search-results__error hide">

								<p>We're sorry, an error occurred while searching for <strong>&ldquo;<span class="term"></span>&rdquo;</strong>.</p>

								<p>Please try again in a few minutes.</p>

							</div>

							<img class="loader hide buffer-top" src="/_duresources/foundation/images/ajax-loader.gif" alt="" />
							

						{% endif %}

						{% if pagination %}
							{% include "molecules-pagination" %}
						{% endif %}
						
					{% endif %}

				</div>
			</div>
		</article>
	{% include "organisms-footer" %}
</div>