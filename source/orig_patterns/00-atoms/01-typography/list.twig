
	{% if list.type %}
		<{{ list.type }}{% if list.styleModifier %} class="{{ list.styleModifier }}"{% endif %}>
	{% endif %}
	{% if not list.type %}
		<ul{% if list.styleModifier %} class="{{ list.styleModifier }}"{% endif %} >
	{% endif %}
		{% for item in list.items %}
		    <li{% if search_filter_on %} class="search-filter-on"{% endif %}>
                {% if item.fact_title %}<p class="stat">{{ item.fact_title|raw }}</p>{% endif %}
                {% if not item.fact_title %}{{ item.title|raw }} {% endif %}
                {% if item.fact_text %} {{ item.fact_text|raw }} {% endif %}
            </li>
		{% endfor %}
	{% if list.type %}
		</{{ list.type }}>
	{% endif %}
	{% if not list.type %}
		</ul>
	{% endif %}
